{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "My shining Nuxt.js project",
  "author": "Kouki Uchida",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
